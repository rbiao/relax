{
    "dev": {
        "images": "./src/images/*.*",
        "imagesSprite": "./src/images/slice/*.png",
        "fonts": "./src/fonts/**/*.*",
        "styles": [
            "./src/styles/style.less"
        ],
        "pages": [
            "./src/modules/**/*.html"
        ],
        "js": "./src/js/**"
    },
    "output": {
        "fonts": "./dist/fonts",
        "images": "./dist/images",
        "styles": "./dist/styles",
        "pages": "./dist/modules",
        "js": "./dist/js"
    },


    "src": {
        "images": "./src/images/*.*",
        "imagesSprite": "./src/images/slice/*.png",
        "fonts": "./src/fonts/**/*.*",
        "styles": [
            "./src/styles/style.less"
        ],
        "pages": [
            "./src/modules/**/*.html"
        ],
        "js": "./src/js/**"
    },
    "develop": {
        "dest": "./dist/develop",
        "output": {
            "fonts": "./dist/develop/fonts",
            "images": "./dist/develop/images",
            "styles": "./dist/develop/styles",
            "pages": "./dist/develop/modules",
            "js": "./dist/develop/js"
        }
    },
    "production": {
        "dest": "./dist/production",
        "output": {
            "fonts": "./dist/production/fonts",
            "images": "./dist/production/images",
            "styles": "./dist/production/styles",
            "pages": "./dist/production/modules",
            "js": "./dist/production/js"
        }
    }
}
